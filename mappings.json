{
  "title": "Norwegian PC Layout",
  "rules": [
    {
      "description": "@ from right-alt + 2",
      "manipulators": [
        {
          "from": {
            "key_code": "2",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "backslash"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "$ from right-alt + 4",
      "manipulators": [
        {
          "from": {
            "key_code": "4",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "4",
              "modifiers": [
                "shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "€ from right-alt + 5",
      "manipulators": [
        {
          "from": {
            "key_code": "5",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "4",
              "modifiers": [
                "right_alt"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "| from non_us_backslash",
      "manipulators": [
        {
          "from": {
            "key_code": "non_us_backslash"
          },
          "to": [
            {
              "key_code": "7",
              "modifiers": [
                "right_alt"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "{ from right-alt + 7",
      "manipulators": [
        {
          "from": {
            "key_code": "7",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "8",
              "modifiers": [
                "shift",
                "option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "} from right-alt + 0",
      "manipulators": [
        {
          "from": {
            "key_code": "0",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": [
                "shift",
                "option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "´ from right-alt + equal-sign",
      "manipulators": [
        {
          "from": {
            "key_code": "equal_sign",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "\\ from equal_sign",
      "manipulators": [
        {
          "from": {
            "key_code": "equal_sign"
          },
          "to": [
            {
              "key_code": "7",
              "modifiers": [
                "right_shift",
                "right_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "' from backslash",
      "manipulators": [
        {
          "from": {
            "key_code": "backslash"
          },
          "to": [
            {
              "key_code": "non_us_backslash"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "~ without second press",
      "manipulators": [
        {
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": [
                "left_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Swap Left Command/Control",
      "manipulators": [
        {
          "from": {
            "key_code": "left_command"
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "left_control"
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Swap Right Command/Option",
      "manipulators": [
        {
          "from": {
            "key_code": "right_command"
          },
          "to": [
            {
              "key_code": "right_option"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "right_option"
          },
          "to": [
            {
              "key_code": "right_command"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
